{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "types": [
            "node"
        ],
        "noEmit": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["**/*.ts"],
}
